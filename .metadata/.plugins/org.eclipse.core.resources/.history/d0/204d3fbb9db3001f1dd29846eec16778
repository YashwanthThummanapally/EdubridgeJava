package collections;

import java.util.LinkedList;

public class LinkedListDemo {
	public static void main(String[] args) {
		LinkedList ll = new LinkedList();
		ll.add(10);
		ll.add(20);
		ll.add("Java");
		ll.add("Python");
		ll.add(25000.0);
		ll.add(35000.0);
		
		System.out.println(ll.size());
		System.out.println(ll);
		
		ll.add(2, "Angular");
		System.out.println(ll.size());
		System.out.println(ll);

	}	
}
