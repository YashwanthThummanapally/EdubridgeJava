// creating user de

public class ThreadDemo {
	public static void main(String[] args) {
		JavaThread javaThread = new JavaThread();
//		javaThread.start(); // begin thread execution
		javaThread.run(); // new thread wont be created
		
		// main thread job
		for(int i=1; i<=10; i++) {
			System.out.println("Main "+i);
		}
	}
}
